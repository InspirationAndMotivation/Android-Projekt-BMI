<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent">


    <TextView
            android:text="Only a small beer belly. \nIt`s okay.\nAlmost.\n(For your partner no.)"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginTop = "160dp"
            android:layout_centerHorizontal="true"
            android:id="@+id/textView5"
            android:textAppearance="@style/TextAppearance.Design.Counter" android:fontFamily="serif-monospace"
            android:textSize="18sp" android:textAlignment="center" android:textColor="@android:color/background_dark"

    />
    <ImageView
            android:layout_width="200dp"
            android:layout_height="245dp"
            app:srcCompat="@drawable/over" android:id="@+id/imageView3"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textView5"
            android:layout_marginTop="30dp"
    />
    <Button
            android:text="Back"
            android:layout_width="115dp"
            android:layout_height="50dp"
            android:id="@+id/button3"
            android:layout_below="@id/textView5"
            android:layout_marginTop="130dp"
            android:layout_marginLeft="60dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse"
            android:onClick="GoBack" android:textAlignment="center"/>
</RelativeLayout>